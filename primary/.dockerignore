# Prevent version control files from being copied
.git
.gitignore

# Prevent local environment variables from being copied
.env
.env.*  # in case you're using .env.local, .env.production, etc.

# Node modules shouldn't be copied; let Docker install them
node_modules

# System files and logs
npm-debug.log
yarn-debug.log
yarn-error.log

# Docker-specific
Dockerfile
.dockerignore
